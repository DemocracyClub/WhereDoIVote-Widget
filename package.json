{
  "name": "wdiv-widget",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "es6-shim": "^0.35.5",
    "react": "^16.9.0",
    "react-app-polyfill": "^1.0.2",
    "react-dom": "^16.9.0",
    "sinon": "^7.4.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.2.0",
    "coveralls": "^3.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "jest-enzyme": "^7.1.0",
    "prettier": "^1.18.2",
    "react-scripts": "3.1.1",
    "react-test-renderer": "^16.9.0",
    "rewire": "^4.0.1"
  },
  "scripts": {
    "start:prod": "REACT_APP_API=prod react-scripts start",
    "start:sandbox": "REACT_APP_API=sandbox react-scripts start",
    "start:mock": "REACT_APP_API=mock react-scripts start",
    "start": "npm run start:prod",
    "build": "node ./build-without-chunks.js",
    "test": "REACT_APP_API=sandbox react-scripts test --env=jsdom",
    "lint": "eslint \"./src/**/*.js\"",
    "prettier": "prettier --write \"./src/**/*.js\"",
    "prettier:check": "prettier --check \"./src/**/*.js\"",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "coveralls": "cat ./coverage/lcov.info | node ./node_modules/.bin/coveralls",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 9",
    "not op_mini all"
  ],
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/test-utils/"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/src/test-utils/"
    ]
  }
}
