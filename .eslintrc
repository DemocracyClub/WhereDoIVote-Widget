{
    "parser": "babel-eslint",
    "plugins": ["prettier", "react"],
    "rules": {
        "prettier/prettier": "error"
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2015
    },
    "env": {
        "browser": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended"]
}
